{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "linebreak-style": "off",
        "no-unused-vars": "warn",
        "import/no-dynamic-require": "warm",
        "consistent-return": 1
    }
}
